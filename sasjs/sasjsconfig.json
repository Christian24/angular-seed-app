{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": [
    "sasjs/macros"
  ],
  "programFolders": [],
  "serviceConfig": {
    "serviceFolders": [
      "sasjs/services/common",
      "sasjs/services/files"
    ],
    "initProgram": "sasjs/services/serviceinit.sas"
  },
  "streamConfig": {
    "streamLogo": "assets/angular-logo.png",
    "streamServiceName": "Angular",
    "streamWeb": false,
    "streamWebFolder": "web",
    "webSourcePath": "dist"
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverUrl": "",
      "serverType": "SASVIYA",
      "appLoc": "/Public/app/angular-seed-app",
      "contextName": "SAS Job Execution compute context"
    },
    {
      "name": "sas9",
      "serverUrl": "",
      "serverType": "SAS9",
      "appLoc": "/Public/app/angular-seed-app",
      "deployConfig": {
        "deployServicePack": true
      },
      "serverName": "SASApp",
      "repositoryName": "Foundation"
    },
    {
      "name": "server",
      "serverUrl": " ",
      "serverType": "SASJS",
      "appLoc": "/Public/app/angular-seed-app"
    }
  ]
}